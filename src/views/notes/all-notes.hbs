<div class="row">
    {{#each notes}}
        <div class="col-md-3">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title d-flex justify-content-between align-items-center">
                        {{title}} <a href="#"><i class="fas fa-edit"></i></a>
                    </h4>
                    <p>{{descripcion}}</p>
                    <p>{{date}}</p>
                    <button class="btn btn-danger btn-block ">
                        Delete
                    </button>
                </div>
            </div>
        </div>
    {{/each}}
</div>